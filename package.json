    "babel-plugin-transform-object-rest-spread": "^6.1.18",
    "babel-preset-es2015-node5": "^1.1.0",
    "babel-preset-es2015-rollup": "^1.0.0",
    "body-parser": "^1.14.1",
    "colors": "^1.1.2",
    "compression": "^1.6.0",
    "cors": "^2.7.1",
    "errorhandler": "^1.4.2",
    "express": "^4.13.3",
    "express-hbs": "*",
    "morgan": "^1.6.1",
    "request-promise": "^1.0.2",
    "require-dir": "^0.3.0",
    "rollup": "^0.21.0",
    "rollup-plugin-babel": "^2.1.0",
    "static-expiry": ">=0.0.10"
